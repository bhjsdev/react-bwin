  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhjsdev/react-bwin.git"
  },
  "keywords": [
    "react",
    "drag",
    "drop",
    "resize",
    "window-manager",
    "tiling-window-manager"
  ],
  "author": "Bhjs Dev <bhjsdev@gmail.com>",
  "license": "MIT",
  "bugs": "https://github.com/bhjsdev/react-bwin/issues",
